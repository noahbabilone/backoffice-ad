<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <meta name="author" content="Yann"/>

    <title>{% block title %}42Consulting{% endblock %}</title>
    {#{% block stylesheets %}{% endblock %}#}
    {#<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>#}
     <link rel="icon" type="image/png" href="{{ asset('images/favicon/favicon-96x96.png') }}"
          sizes="96x96">
    {#<link rel="icon" type="image/png" href="{{ asset('images/favicon/favicon-32x32.png') }}"#}
          {#sizes="32x32">#}
    {#<link rel="icon" type="image/png" href="{{ asset('images/favicon/favicon-16x16.png') }}"#}
          {#sizes="16x16">#}
    {% block stylesheets %}
        {% stylesheets
        'bundles/ad/themes/global/plugins/bootstrap/css/bootstrap.min.css'
        'bundles/ad/themes/global/plugins/font-awesome/css/font-awesome.min.css'
         filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}
    {% endblock %}
</head>
<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">
{% block body %}{% endblock %}
{% block javascripts %}
    {% javascripts
    'https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js'
    'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js'
    'bundles/fosjsrouting/js/router.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
<script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>

</body>
</html>
